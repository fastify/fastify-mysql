    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-mysql/issues"
  },
  "homepage": "https://github.com/fastify/fastify-mysql#readme",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/fastify"
    },
    {
